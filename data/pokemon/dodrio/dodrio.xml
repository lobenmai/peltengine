    <anim_back>pokemon/dodrio/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>dodrio</name>
	<!-- proper name -->
	<show_name>Dodrio</show_name>
	<!-- number of the pokemon -->
	<num>85</num>
	<!-- type of exp growth -->
	<exp>medfast</exp>
	<!-- species name -->
	<species>Triple Bird</species>
	<type>normal|flying</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>71</height> <!-- height of pokemon in inches -->
		<weight>187.8</weight> <!-- weight of pokemon in pounds -->
		<desc>An odd species that is rarely found. The three heads respectively represent joy, sadness, and anger.</desc>
		<color>brown</color>
		<num>85</num>
	</pokedex>
	<ability>
		<normal>run_away</normal> <!-- various abilities -->
		<normal>early_bird</normal> <!-- are chosen from at random when generated -->
		<hidden></hidden>
	</ability>
	<catch>45</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>flying</group> <!-- breeding group of pokemon -->
		<cycles>20</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>158</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|2|0|0|0|0</ev> <!-- ev yield data -->
		<stats>60|110|70|60|60|100</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">peck</move>
			<move level="0">growl</move>
			<move level="0">pursuit</move>
			<move level="0">fury_attack</move>
			<move level="9">pursuit</move>
			<move level="13">fury_attack</move>
			<move level="21">tri_attack</move>
			<move level="25">rage</move>
			<move level="38">uproar</move>
			<move level="47">drill_peck</move>
			<move level="60">agility</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>