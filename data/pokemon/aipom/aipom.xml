<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/aipom/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/aipom/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>aipom</name>
	<!-- proper name -->
	<show_name>Aipom</show_name>
	<!-- number of the pokemon -->
	<num>190</num>
	<!-- type of exp growth -->
	<exp>fast</exp>
	<!-- species name -->
	<species>Long Tail</species>
	<type>normal</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>31</height> <!-- height of pokemon in inches -->
		<weight>25.4</weight> <!-- weight of pokemon in pounds -->
		<desc>It lives atop tall trees. When leaping from branch to branch, it deftly uses its tail for balance.</desc>
		<color>purple</color>
		<num>190</num>
	</pokedex>
	<ability>
		<normal>run_away</normal> <!-- various abilities -->
		<normal>pickup</normal> <!-- are chosen from at random when generated -->
		<hidden></hidden>
	</ability>
	<catch>45</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>ground</group> <!-- breeding group of pokemon -->
		<cycles>20</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>94</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|0|0|0|0|1</ev> <!-- ev yield data -->
		<stats>55|70|55|40|55|85</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">scratch</move>
			<move level="0">tail_whip</move>
			<move level="6">sand-attack</move>
			<move level="13">mist</move>
			<move level="18">baton_pass</move>
			<move level="25">drill_peck</move>
			<move level="31">fury_swipes</move>
			<move level="38">swift</move>
			<move level="43">screech</move>
			<move level="50">agility</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>