<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/caterpie/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/caterpie/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>caterpie</name>
	<!-- proper name -->
	<show_name>Caterpie</show_name>
	<!-- number of the pokemon -->
	<num>10</num>
	<!-- type of exp growth -->
	<exp>medfast</exp>
	<!-- species name -->
	<species>Worm</species>
	<type>bug</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>12</height> <!-- height of pokemon in inches -->
		<weight>6.4</weight> <!-- weight of pokemon in pounds -->
		<desc>It is covered with a green skin. When it grows, it sheds the skin, covers itself with silk, and becomes a cocoon.</desc>
		<color>green</color>
		<num>10</num>
	</pokedex>
	<ability>
		<normal>shield_dust</normal> <!-- various abilities -->
		<hidden></hidden>
	</ability>
	<catch>255</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>bug</group> <!-- breeding group of pokemon -->
		<cycles>15</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>53</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>1|0|0|0|0|0</ev> <!-- ev yield data -->
		<stats>45|30|35|20|20|45</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">tackle</move>
			<move level="0">string_shot</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>