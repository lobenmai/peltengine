<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/magnemite/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/magnemite/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>magnemite</name>
	<!-- proper name -->
	<show_name>Magnemite</show_name>
	<!-- number of the pokemon -->
	<num>81</num>
	<!-- type of exp growth -->
	<exp>medfast</exp>
	<!-- species name -->
	<species>Magnet</species>
	<type>electric|steel</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>12</height> <!-- height of pokemon in inches -->
		<weight>13.2</weight> <!-- weight of pokemon in pounds -->
		<desc>It moves while constantly hovering. It discharges THUNDER WAVE and so on from the units at its sides.</desc>
		<color>gray</color>
		<num>81</num>
	</pokedex>
	<ability>
		<normal>magnet_pull</normal> <!-- various abilities -->
		<normal>sturdy</normal> <!-- are chosen from at random when generated -->
		<hidden></hidden>
	</ability>
	<catch>190</catch>
	<gender>-1</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>mineral</group> <!-- breeding group of pokemon -->
		<cycles>20</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>89</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|0|0|1|0|0</ev> <!-- ev yield data -->
		<stats>25|35|70|95|55|45</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">hyper_beam</move>
			<move level="0">tackle</move>
			<move level="6">thundershock</move>
			<move level="11">supersonic</move>
			<move level="16">sonicboom</move>
			<move level="21">thunder_wave</move>
			<move level="26">spark</move>
			<move level="32">lock-on</move>
			<move level="38">swift</move>
			<move level="44">screech</move>
			<move level="50">zap_cannon</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>