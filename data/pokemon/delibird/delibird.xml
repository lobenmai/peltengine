<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/delibird/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/delibird/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>delibird</name>
	<!-- proper name -->
	<show_name>Delibird</show_name>
	<!-- number of the pokemon -->
	<num>225</num>
	<!-- type of exp growth -->
	<exp>fast</exp>
	<!-- species name -->
	<species>Delivery</species>
	<type>ice|flying</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>35</height> <!-- height of pokemon in inches -->
		<weight>35.3</weight> <!-- weight of pokemon in pounds -->
		<desc>It nests at the edge of sharp cliffs. It spends all day carrying food to its awaiting chicks.</desc>
		<color>red</color>
		<num>225</num>
	</pokedex>
	<ability>
		<normal>vital_spirit</normal> <!-- various abilities -->
		<normal>hustle</normal> <!-- are chosen from at random when generated -->
		<hidden></hidden>
	</ability>
	<catch>45</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>water_1|ground</group> <!-- breeding group of pokemon -->
		<cycles>20</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>183</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|0|0|0|0|1</ev> <!-- ev yield data -->
		<stats>45|55|45|65|45|75</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">present</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>