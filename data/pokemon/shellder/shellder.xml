<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/shellder/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/shellder/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>shellder</name>
	<!-- proper name -->
	<show_name>Shellder</show_name>
	<!-- number of the pokemon -->
	<num>90</num>
	<!-- type of exp growth -->
	<exp>slow</exp>
	<!-- species name -->
	<species>Bivalve</species>
	<type>water</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>12</height> <!-- height of pokemon in inches -->
		<weight>8.8</weight> <!-- weight of pokemon in pounds -->
		<desc>It is encased in a shell that is harder than diamond. Inside, however, it is surprisingly tender.</desc>
		<color>purple</color>
		<num>90</num>
	</pokedex>
	<ability>
		<normal>shell_armor</normal> <!-- various abilities -->
		<hidden></hidden>
	</ability>
	<catch>190</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>water_3</group> <!-- breeding group of pokemon -->
		<cycles>20</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>97</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|0|1|0|0|0</ev> <!-- ev yield data -->
		<stats>30|65|100|45|25|40</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">tackle</move>
			<move level="0">withdraw</move>
			<move level="8">poisonpowder</move>
			<move level="15">supersonic</move>
			<move level="22">aurora_beam</move>
			<move level="29">protect</move>
			<move level="36">leer</move>
			<move level="43">clamp</move>
			<move level="50">ice_beam</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>