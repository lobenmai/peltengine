<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/paras/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/paras/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>paras</name>
	<!-- proper name -->
	<show_name>Paras</show_name>
	<!-- number of the pokemon -->
	<num>46</num>
	<!-- type of exp growth -->
	<exp>medfast</exp>
	<!-- species name -->
	<species>Mushroom</species>
	<type>bug|grass</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>12</height> <!-- height of pokemon in inches -->
		<weight>11.9</weight> <!-- weight of pokemon in pounds -->
		<desc>Growing out of the bug's back are mushrooms called tochukaso. The mushrooms grow with the bug host.</desc>
		<color>red</color>
		<num>46</num>
	</pokedex>
	<ability>
		<normal>effect_spore</normal> <!-- various abilities -->
		<hidden></hidden>
	</ability>
	<catch>190</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>bug|plant</group> <!-- breeding group of pokemon -->
		<cycles>20</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>70</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|1|0|0|0|0</ev> <!-- ev yield data -->
		<stats>35|70|55|45|55|25</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">scratch</move>
			<move level="7">stun_spore</move>
			<move level="13">poisonpowder</move>
			<move level="19">leech_life</move>
			<move level="25">spore</move>
			<move level="31">slash</move>
			<move level="37">growth</move>
			<move level="43">giga_drain</move>
			<move level="49">hydro_pump</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>