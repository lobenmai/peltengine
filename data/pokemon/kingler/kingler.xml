			<move level="0">metal_claw</move>
			<move level="0">bubble</move>
			<move level="0">leer</move>
			<move level="0">vicegrip</move>
			<move level="0">harden</move>
			<move level="5">leer</move>
			<move level="12">vicegrip</move>
			<move level="16">harden</move>
			<move level="23">thunderbolt</move>
			<move level="27">stomp</move>
			<move level="38">guillotine</move>
			<move level="42">protect</move>
			<move level="57">crabhammer</move>
			<move level="65">flail</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>