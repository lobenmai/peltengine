<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/onix/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/onix/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>onix</name>
	<!-- proper name -->
	<show_name>Onix</show_name>
	<!-- number of the pokemon -->
	<num>95</num>
	<!-- type of exp growth -->
	<exp>medfast</exp>
	<!-- species name -->
	<species>Rock Snake</species>
	<type>rock|ground</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>346</height> <!-- height of pokemon in inches -->
		<weight>463.0</weight> <!-- weight of pokemon in pounds -->
		<desc>It usually lives underground. It searches for food while boring its way through the ground at 50 miles per hour.</desc>
		<color>gray</color>
		<num>95</num>
	</pokedex>
	<ability>
		<normal>rock_head</normal> <!-- various abilities -->
		<normal>sturdy</normal> <!-- are chosen from at random when generated -->
		<hidden></hidden>
	</ability>
	<catch>45</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>mineral</group> <!-- breeding group of pokemon -->
		<cycles>25</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>108</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|0|1|0|0|0</ev> <!-- ev yield data -->
		<stats>35|45|160|30|45|70</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">tackle</move>
			<move level="0">screech</move>
			<move level="8">bind</move>
			<move level="12">rock_throw</move>
			<move level="19">harden</move>
			<move level="23">rage</move>
			<move level="30">dragonbreath</move>
			<move level="34">sandstorm</move>
			<move level="41">slam</move>
			<move level="45">iron_tail</move>
			<move level="52">mega_drain</move>
			<move level="56">double-edge</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>