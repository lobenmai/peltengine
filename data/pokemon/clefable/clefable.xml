<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/clefable/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/clefable/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>clefable</name>
	<!-- proper name -->
	<show_name>Clefable</show_name>
	<!-- number of the pokemon -->
	<num>36</num>
	<!-- type of exp growth -->
	<exp>fast</exp>
	<!-- species name -->
	<species>Fairy</species>
	<type>normal</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>51</height> <!-- height of pokemon in inches -->
		<weight>88.2</weight> <!-- weight of pokemon in pounds -->
		<desc>It has an acute sense of hearing. It can easily hear a pin being dropped nearly 1,100 yards away.</desc>
		<color>pink</color>
		<num>36</num>
	</pokedex>
	<ability>
		<normal>cute_charm</normal> <!-- various abilities -->
		<hidden></hidden>
	</ability>
	<catch>25</catch>
	<gender>25.0</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>fairy</group> <!-- breeding group of pokemon -->
		<cycles>10</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>129</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>3|0|0|0|0|0</ev> <!-- ev yield data -->
		<stats>95|70|73|85|90|60</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">sing</move>
			<move level="0">doubleslap</move>
			<move level="0">minimize</move>
			<move level="0">metronome</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>