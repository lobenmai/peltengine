<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/articuno/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/articuno/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>articuno</name>
	<!-- proper name -->
	<show_name>Articuno</show_name>
	<!-- number of the pokemon -->
	<num>144</num>
	<!-- type of exp growth -->
	<exp>slow</exp>
	<!-- species name -->
	<species>Freeze</species>
	<type>ice|flying</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>67</height> <!-- height of pokemon in inches -->
		<weight>122.1</weight> <!-- weight of pokemon in pounds -->
		<desc>One of the legendary bird Pok{ae}mon. With its long tail trailing behind, its flying form is magnificent.</desc>
		<color>blue</color>
		<num>144</num>
	</pokedex>
	<ability>
		<normal>pressure</normal> <!-- various abilities -->
		<hidden></hidden>
	</ability>
	<catch>3</catch>
	<gender>-1</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>no_egg</group> <!-- breeding group of pokemon -->
		<cycles>80</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>215</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|0|0|0|3|0</ev> <!-- ev yield data -->
		<stats>90|85|100|95|125|85</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">gust</move>
			<move level="0">powder_snow</move>
			<move level="13">mist</move>
			<move level="25">agility</move>
			<move level="37">mind_reader</move>
			<move level="49">ice_beam</move>
			<move level="61">reflect</move>
			<move level="73">blizzard</move>
			<move level="85">leech_seed</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>