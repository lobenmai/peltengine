	<!-- internal name of pokemon -->
	<name>ledian</name>
	<!-- proper name -->
	<show_name>Ledian</show_name>
	<!-- number of the pokemon -->
	<num>166</num>
	<!-- type of exp growth -->
	<exp>fast</exp>
	<!-- species name -->
	<species>Five Star</species>
	<type>bug|flying</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>55</height> <!-- height of pokemon in inches -->
		<weight>78.5</weight> <!-- weight of pokemon in pounds -->
