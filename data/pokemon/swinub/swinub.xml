<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/swinub/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/swinub/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>swinub</name>
	<!-- proper name -->
	<show_name>Swinub</show_name>
	<!-- number of the pokemon -->
	<num>220</num>
	<!-- type of exp growth -->
	<exp>slow</exp>
	<!-- species name -->
	<species>Pig</species>
	<type>ice|ground</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>16</height> <!-- height of pokemon in inches -->
		<weight>14.3</weight> <!-- weight of pokemon in pounds -->
		<desc>If it smells something enticing, it dashes off headlong to find the source of the aroma.</desc>
		<color>brown</color>
		<num>220</num>
	</pokedex>
	<ability>
		<normal>oblivious</normal> <!-- various abilities -->
		<hidden></hidden>
	</ability>
	<catch>225</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>ground</group> <!-- breeding group of pokemon -->
		<cycles>20</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>78</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|1|0|0|0|0</ev> <!-- ev yield data -->
		<stats>50|50|40|30|30|50</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">tackle</move>
			<move level="0">psybeam</move>
			<move level="10">powder_snow</move>
			<move level="19">endure</move>
			<move level="28">take_down</move>
			<move level="37">mist</move>
			<move level="46">blizzard</move>
			<move level="55">amnesia</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>