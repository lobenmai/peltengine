<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/arbok/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/arbok/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>arbok</name>
	<!-- proper name -->
	<show_name>Arbok</show_name>
	<!-- number of the pokemon -->
	<num>24</num>
	<!-- type of exp growth -->
	<exp>medfast</exp>
	<!-- species name -->
	<species>Cobra</species>
	<type>poison</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>138</height> <!-- height of pokemon in inches -->
		<weight>143.3</weight> <!-- weight of pokemon in pounds -->
		<desc>The pattern on its belly appears to be a frightening face. Weak foes will flee just at the sight of the pattern.</desc>
		<color>purple</color>
		<num>24</num>
	</pokedex>
	<ability>
		<normal>intimidate</normal> <!-- various abilities -->
		<normal>shed_skin</normal> <!-- are chosen from at random when generated -->
		<hidden></hidden>
	</ability>
	<catch>90</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>ground|dragon</group> <!-- breeding group of pokemon -->
		<cycles>20</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>147</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|2|0|0|0|0</ev> <!-- ev yield data -->
		<stats>60|85|69|65|79|80</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">wrap</move>
			<move level="0">leer</move>
			<move level="0">poison_sting</move>
			<move level="0">bite</move>
			<move level="8">poison_sting</move>
			<move level="13">bite</move>
			<move level="20">glare</move>
			<move level="28">screech</move>
			<move level="38">acid</move>
			<move level="46">stockpile</move>
			<move level="46">psycho_boost</move>
			<move level="46">spit_up</move>
			<move level="56">haze</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>