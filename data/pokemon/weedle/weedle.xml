<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/weedle/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/weedle/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>weedle</name>
	<!-- proper name -->
	<show_name>Weedle</show_name>
	<!-- number of the pokemon -->
	<num>13</num>
	<!-- type of exp growth -->
	<exp>medfast</exp>
	<!-- species name -->
	<species>Hairy Bug</species>
	<type>bug|poison</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>12</height> <!-- height of pokemon in inches -->
		<weight>7.1</weight> <!-- weight of pokemon in pounds -->
		<desc>Often found in forests and grasslands. It has a sharp, toxic barb of around two inches on top of its head.</desc>
		<color>brown</color>
		<num>13</num>
	</pokedex>
	<ability>
		<normal>shield_dust</normal> <!-- various abilities -->
		<hidden></hidden>
	</ability>
	<catch>255</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>bug</group> <!-- breeding group of pokemon -->
		<cycles>15</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>52</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|0|0|0|0|1</ev> <!-- ev yield data -->
		<stats>40|35|30|20|20|50</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">poison_sting</move>
			<move level="0">string_shot</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>