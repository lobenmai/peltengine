<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/natu/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/natu/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>natu</name>
	<!-- proper name -->
	<show_name>Natu</show_name>
	<!-- number of the pokemon -->
	<num>177</num>
	<!-- type of exp growth -->
	<exp>medfast</exp>
	<!-- species name -->
	<species>Tiny Bird</species>
	<type>psychic|flying</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>8</height> <!-- height of pokemon in inches -->
		<weight>4.4</weight> <!-- weight of pokemon in pounds -->
		<desc>It usually forages for food on the ground but may, on rare occasions, hop onto branches to peck at shoots.</desc>
		<color>green</color>
		<num>177</num>
	</pokedex>
	<ability>
		<normal>synchronize</normal> <!-- various abilities -->
		<normal>early_bird</normal> <!-- are chosen from at random when generated -->
		<hidden></hidden>
	</ability>
	<catch>190</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>flying</group> <!-- breeding group of pokemon -->
		<cycles>20</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>73</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|0|0|1|0|0</ev> <!-- ev yield data -->
		<stats>40|50|45|70|45|70</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">peck</move>
			<move level="0">leer</move>
			<move level="10">night_shade</move>
			<move level="20">teleport</move>
			<move level="30">wing_attack</move>
			<move level="30">future_sight</move>
			<move level="40">confuse_ray</move>
			<move level="50">psychic</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>