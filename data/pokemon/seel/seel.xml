	<name>seel</name>
	<!-- proper name -->
	<show_name>Seel</show_name>
	<!-- number of the pokemon -->
	<num>86</num>
	<!-- type of exp growth -->
	<exp>medfast</exp>
	<!-- species name -->
	<species>Sea Lion</species>
	<type>water</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>43</height> <!-- height of pokemon in inches -->
		<weight>198.4</weight> <!-- weight of pokemon in pounds -->
		<desc>Covered with light blue fur, its hide is thick and tough. It is active in bitter cold of minus 40 degrees Fahrenheit.</desc>
