<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/pidgeot/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/pidgeot/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>pidgeot</name>
	<!-- proper name -->
	<show_name>Pidgeot</show_name>
	<!-- number of the pokemon -->
	<num>18</num>
	<!-- type of exp growth -->
	<exp>medslow</exp>
	<!-- species name -->
	<species>Bird</species>
	<type>normal|flying</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>59</height> <!-- height of pokemon in inches -->
		<weight>87.1</weight> <!-- weight of pokemon in pounds -->
		<desc>It spreads its gorgeous wings widely to intimidate enemies. It races through the skies at Mach-2 speed.</desc>
		<color>brown</color>
		<num>18</num>
	</pokedex>
	<ability>
		<normal>keen_eye</normal> <!-- various abilities -->
		<hidden></hidden>
	</ability>
	<catch>45</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>flying</group> <!-- breeding group of pokemon -->
		<cycles>15</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>172</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|0|0|0|0|3</ev> <!-- ev yield data -->
		<stats>83|80|75|70|70|91</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">tackle</move>
			<move level="0">sand-attack</move>
			<move level="0">gust</move>
			<move level="0">quick_attack</move>
			<move level="5">sand-attack</move>
			<move level="9">gust</move>
			<move level="13">quick_attack</move>
			<move level="20">whirlwind</move>
			<move level="27">wing_attack</move>
			<move level="34">twineedle</move>
			<move level="48">agility</move>
			<move level="62">mirror_move</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>