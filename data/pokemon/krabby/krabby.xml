<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/krabby/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/krabby/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>krabby</name>
	<!-- proper name -->
	<show_name>Krabby</show_name>
	<!-- number of the pokemon -->
	<num>98</num>
	<!-- type of exp growth -->
	<exp>medfast</exp>
	<!-- species name -->
	<species>River Crab</species>
	<type>water</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>16</height> <!-- height of pokemon in inches -->
		<weight>14.3</weight> <!-- weight of pokemon in pounds -->
		<desc>It can be found near the sea. The large pincers grow back if they are torn out of their sockets.</desc>
		<color>red</color>
		<num>98</num>
	</pokedex>
	<ability>
		<normal>hyper_cutter</normal> <!-- various abilities -->
		<normal>shell_armor</normal> <!-- are chosen from at random when generated -->
		<hidden></hidden>
	</ability>
	<catch>225</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>water_3</group> <!-- breeding group of pokemon -->
		<cycles>20</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>115</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|1|0|0|0|0</ev> <!-- ev yield data -->
		<stats>30|105|90|25|25|50</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">bubble</move>
			<move level="5">leer</move>
			<move level="12">vicegrip</move>
			<move level="16">harden</move>
			<move level="23">thunderbolt</move>
			<move level="27">stomp</move>
			<move level="34">guillotine</move>
			<move level="38">protect</move>
			<move level="45">crabhammer</move>
			<move level="49">flail</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>