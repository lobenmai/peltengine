<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/politoed/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/politoed/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>politoed</name>
	<!-- proper name -->
	<show_name>Politoed</show_name>
	<!-- number of the pokemon -->
	<num>186</num>
	<!-- type of exp growth -->
	<exp>medslow</exp>
	<!-- species name -->
	<species>Frog</species>
	<type>water</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>43</height> <!-- height of pokemon in inches -->
		<weight>74.7</weight> <!-- weight of pokemon in pounds -->
		<desc>Whenever three or more of these get together, they sing in a loud voice that sounds like bellowing.</desc>
		<color>green</color>
		<num>186</num>
	</pokedex>
	<ability>
		<normal>water_absorb</normal> <!-- various abilities -->
		<normal>damp</normal> <!-- are chosen from at random when generated -->
		<hidden></hidden>
	</ability>
	<catch>45</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>water_1</group> <!-- breeding group of pokemon -->
		<cycles>20</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>185</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|0|0|0|3|0</ev> <!-- ev yield data -->
		<stats>90|75|75|90|100|70</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">water_gun</move>
			<move level="0">hypnosis</move>
			<move level="0">doubleslap</move>
			<move level="0">perish_song</move>
			<move level="35">perish_song</move>
			<move level="51">swagger</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>