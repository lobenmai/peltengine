<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/clefairy/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/clefairy/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>clefairy</name>
	<!-- proper name -->
	<show_name>Clefairy</show_name>
	<!-- number of the pokemon -->
	<num>35</num>
	<!-- type of exp growth -->
	<exp>fast</exp>
	<!-- species name -->
	<species>Fairy</species>
	<type>normal</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>24</height> <!-- height of pokemon in inches -->
		<weight>16.5</weight> <!-- weight of pokemon in pounds -->
		<desc>Its adorable appearance makes it popular as a pet. However, it is rare and difficult to find.</desc>
		<color>pink</color>
		<num>35</num>
	</pokedex>
	<ability>
		<normal>cute_charm</normal> <!-- various abilities -->
		<hidden></hidden>
	</ability>
	<catch>150</catch>
	<gender>25.0</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>fairy</group> <!-- breeding group of pokemon -->
		<cycles>10</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>68</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>2|0|0|0|0|0</ev> <!-- ev yield data -->
		<stats>70|45|48|60|65|35</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">pound</move>
			<move level="0">growl</move>
			<move level="5">encore</move>
			<move level="9">sing</move>
			<move level="13">doubleslap</move>
			<move level="17">scratch</move>
			<move level="21">minimize</move>
			<move level="25">defense_curl</move>
			<move level="29">metronome</move>
			<move level="33">submission</move>
			<move level="37">moonlight</move>
			<move level="41">light_screen</move>
			<move level="45">flamethrower</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>