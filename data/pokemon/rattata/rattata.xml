<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/rattata/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/rattata/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>rattata</name>
	<!-- proper name -->
	<show_name>Rattata</show_name>
	<!-- number of the pokemon -->
	<num>19</num>
	<!-- type of exp growth -->
	<exp>medfast</exp>
	<!-- species name -->
	<species>Mouse</species>
	<type>normal</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>12</height> <!-- height of pokemon in inches -->
		<weight>7.7</weight> <!-- weight of pokemon in pounds -->
		<desc>Its fangs are long and very sharp. They grow continuously, so it gnaws on hard things to whittle them down.</desc>
		<color>purple</color>
		<num>19</num>
	</pokedex>
	<ability>
		<normal>run_away</normal> <!-- various abilities -->
		<normal>guts</normal> <!-- are chosen from at random when generated -->
		<hidden></hidden>
	</ability>
	<catch>255</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>ground</group> <!-- breeding group of pokemon -->
		<cycles>15</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>57</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|0|0|0|0|1</ev> <!-- ev yield data -->
		<stats>30|56|35|25|35|72</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">tackle</move>
			<move level="0">tail_whip</move>
			<move level="7">quick_attack</move>
			<move level="13">hyper_fang</move>
			<move level="20">focus_energy</move>
			<move level="27">pursuit</move>
			<move level="34">super_fang</move>
			<move level="41">rolling_kick</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>