	<name>togepi</name>
	<!-- proper name -->
	<show_name>Togepi</show_name>
	<!-- number of the pokemon -->
	<num>175</num>
	<!-- type of exp growth -->
	<exp>fast</exp>
	<!-- species name -->
	<species>Spike Ball</species>
	<type>normal</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>12</height> <!-- height of pokemon in inches -->
		<weight>3.3</weight> <!-- weight of pokemon in pounds -->
