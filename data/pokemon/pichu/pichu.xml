<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/pichu/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/pichu/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>pichu</name>
	<!-- proper name -->
	<show_name>Pichu</show_name>
	<!-- number of the pokemon -->
	<num>172</num>
	<!-- type of exp growth -->
	<exp>medfast</exp>
	<!-- species name -->
	<species>Tiny Mouse</species>
	<type>electric</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>12</height> <!-- height of pokemon in inches -->
		<weight>4.4</weight> <!-- weight of pokemon in pounds -->
		<desc>Despite its small size, it can zap even adult humans. However, if it does so, it also surprises itself.</desc>
		<color>yellow</color>
		<num>172</num>
	</pokedex>
	<ability>
		<normal>static</normal> <!-- various abilities -->
		<hidden></hidden>
	</ability>
	<catch>190</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>no_egg</group> <!-- breeding group of pokemon -->
		<cycles>10</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>42</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|0|0|0|0|1</ev> <!-- ev yield data -->
		<stats>20|40|15|35|35|60</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">thundershock</move>
			<move level="0">charm</move>
			<move level="6">tail_whip</move>
			<move level="8">thunder_wave</move>
			<move level="11">sweet_kiss</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>