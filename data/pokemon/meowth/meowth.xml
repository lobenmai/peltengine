<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/meowth/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/meowth/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>meowth</name>
	<!-- proper name -->
	<show_name>Meowth</show_name>
	<!-- number of the pokemon -->
	<num>52</num>
	<!-- type of exp growth -->
	<exp>medfast</exp>
	<!-- species name -->
	<species>Scratch Cat</species>
	<type>normal</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>16</height> <!-- height of pokemon in inches -->
		<weight>9.3</weight> <!-- weight of pokemon in pounds -->
		<desc>All it does is sleep during the daytime. At night, it patrols its territory with its eyes aglow.</desc>
		<color>yellow</color>
		<num>52</num>
	</pokedex>
	<ability>
		<normal>pickup</normal> <!-- various abilities -->
		<hidden></hidden>
	</ability>
	<catch>255</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>ground</group> <!-- breeding group of pokemon -->
		<cycles>20</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>69</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|0|0|0|0|1</ev> <!-- ev yield data -->
		<stats>40|45|35|40|40|90</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">scratch</move>
			<move level="0">growl</move>
			<move level="10">bite</move>
			<move level="18">pay_day</move>
			<move level="25">faint_attack</move>
			<move level="31">screech</move>
			<move level="36">fury_swipes</move>
			<move level="40">slash</move>
			<move level="43">fake_out</move>
			<move level="45">swagger</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>