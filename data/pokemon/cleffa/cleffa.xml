			<!-- level at which move is learned + move id -->
			<move level="0">pound</move>
			<move level="0">charm</move>
			<move level="4">encore</move>
			<move level="8">sing</move>
			<move level="13">sweet_kiss</move>
			<move level="17">earthquake</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>