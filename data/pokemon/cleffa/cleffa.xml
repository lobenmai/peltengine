    <anim_back>pokemon/cleffa/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>cleffa</name>
	<!-- proper name -->
	<show_name>Cleffa</show_name>
	<!-- number of the pokemon -->
	<num>173</num>
	<!-- type of exp growth -->
	<exp>fast</exp>
	<!-- species name -->
	<species>Star Shape</species>
	<type>normal</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>12</height> <!-- height of pokemon in inches -->
		<weight>6.6</weight> <!-- weight of pokemon in pounds -->
		<desc>When numerous meteors illuminate the night sky, sightings of CLEFFA strangely increase.</desc>
		<color>pink</color>
		<num>173</num>
	</pokedex>
	<ability>
		<normal>cute_charm</normal> <!-- various abilities -->
		<hidden></hidden>
	</ability>
	<catch>150</catch>
	<gender>25.0</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>no_egg</group> <!-- breeding group of pokemon -->
		<cycles>10</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>37</exp>
