<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/pikachu/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/pikachu/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>pikachu</name>
	<!-- proper name -->
	<show_name>Pikachu</show_name>
	<!-- number of the pokemon -->
	<num>25</num>
	<!-- type of exp growth -->
	<exp>medfast</exp>
	<!-- species name -->
	<species>Mouse</species>
	<type>electric</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>16</height> <!-- height of pokemon in inches -->
		<weight>13.2</weight> <!-- weight of pokemon in pounds -->
		<desc>It has small electric sacs on both its cheeks. If threatened, it looses electric charges from the sacs.</desc>
		<color>yellow</color>
		<num>25</num>
	</pokedex>
	<ability>
		<normal>static</normal> <!-- various abilities -->
		<hidden></hidden>
	</ability>
	<catch>190</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>ground|fairy</group> <!-- breeding group of pokemon -->
		<cycles>10</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>82</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|0|0|0|0|2</ev> <!-- ev yield data -->
		<stats>35|55|30|50|40|90</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">thundershock</move>
			<move level="0">growl</move>
			<move level="6">tail_whip</move>
			<move level="8">thunder_wave</move>
			<move level="11">quick_attack</move>
			<move level="15">double_team</move>
			<move level="20">slam</move>
			<move level="26">thunderbolt</move>
			<move level="33">agility</move>
			<move level="41">thunder</move>
			<move level="50">light_screen</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>