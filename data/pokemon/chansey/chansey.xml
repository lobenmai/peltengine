<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/chansey/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/chansey/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>chansey</name>
	<!-- proper name -->
	<show_name>Chansey</show_name>
	<!-- number of the pokemon -->
	<num>113</num>
	<!-- type of exp growth -->
	<exp>fast</exp>
	<!-- species name -->
	<species>Egg</species>
	<type>normal</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>43</height> <!-- height of pokemon in inches -->
		<weight>76.3</weight> <!-- weight of pokemon in pounds -->
		<desc>It lays several eggs a day. The eggs are apparently rich in nutrients and extremely delicious.</desc>
		<color>pink</color>
		<num>113</num>
	</pokedex>
	<ability>
		<normal>natural_cure</normal> <!-- various abilities -->
		<normal>serene_grace</normal> <!-- are chosen from at random when generated -->
		<hidden></hidden>
	</ability>
	<catch>30</catch>
	<gender>0.0</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>fairy</group> <!-- breeding group of pokemon -->
		<cycles>40</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>255</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>2|0|0|0|0|0</ev> <!-- ev yield data -->
		<stats>250|5|5|35|105|50</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">pound</move>
			<move level="0">growl</move>
			<move level="5">tail_whip</move>
			<move level="9">fury_attack</move>
			<move level="13">softboiled</move>
			<move level="17">doubleslap</move>
			<move level="23">minimize</move>
			<move level="29">sing</move>
			<move level="35">egg_bomb</move>
			<move level="41">defense_curl</move>
			<move level="49">light_screen</move>
			<move level="57">double-edge</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>