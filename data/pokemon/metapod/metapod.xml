<pokemon_data> <!-- data for one pokemon -->
    <anim_front>pokemon/metapod/anim.xml</anim_front> <!-- path to animation data for front -->
    <anim_back>pokemon/metapod/anim_back.xml</anim_back> <!-- path to animation data for back -->
	<!-- internal name of pokemon -->
	<name>metapod</name>
	<!-- proper name -->
	<show_name>Metapod</show_name>
	<!-- number of the pokemon -->
	<num>11</num>
	<!-- type of exp growth -->
	<exp>medfast</exp>
	<!-- species name -->
	<species>Cocoon</species>
	<type>bug</type>
	<pokedex> <!-- various pokedex metrics -->
		<height>28</height> <!-- height of pokemon in inches -->
		<weight>21.8</weight> <!-- weight of pokemon in pounds -->
		<desc>Even though it is encased in a sturdy shell, the body inside is tender. It can't withstand a harsh attack.</desc>
		<color>green</color>
		<num>11</num>
	</pokedex>
	<ability>
		<normal>shed_skin</normal> <!-- various abilities -->
		<hidden></hidden>
	</ability>
	<catch>120</catch>
	<gender>50</gender> <!-- likelihood of pokemon being male. if -1, pokemon is genderless -->
	<breed>
		<group>bug</group> <!-- breeding group of pokemon -->
		<cycles>15</cycles> <!-- number of cycles necessary -->
	</breed>
	<base> <!-- base yield data -->
		<exp>72</exp>
		<!-- hp|atk|def|sp.atk|sp.def|speed -->
		<ev>0|0|2|0|0|0</ev> <!-- ev yield data -->
		<stats>50|20|55|25|25|30</stats>
	</base>
	<learnset> <!-- learnset of pokemon -->
		<level> <!-- moves learned at a specific level -->
			<!-- level at which move is learned + move id -->
			<move level="0">harden</move>
			<move level="7">harden</move>
		</level>
		<!-- which TMs and HMs this pokemon can learn -->
		<tm></tm>
		<hm></hm>
	</learnset>
</pokemon_data>